[distutils]
index-servers=
    pypi
    test
    digiges-test
    digiges-pypi

[test]
repository:https://test.pypi.org/legacy/
username:{{@@ env['pypi_user'] @@}}
password:{{@@ env['pypi_password'] @@}}

[pypi]
username:{{@@ env['pypi_user'] @@}}
password:{{@@ env['pypi_password'] @@}}


[digiges-test]
repository:https://test.pypi.org/legacy/
username:{{@@ env['digiges_pypi_user'] @@}}
password:{{@@ env['digiges_pypi_password'] @@}}

[digiges-pypi]
repository:https://upload.pypi.org/legacy/
username:{{@@ env['digiges_pypi_user'] @@}}
password:{{@@ env['digiges_pypi_password'] @@}}
